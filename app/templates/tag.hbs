<div class="row">
  {{#each model as |tag|}}
    <div class="col-4 d-inline">
      {{tag.name}}
    </div>
  {{/each}}
</div>

<form {{action "addTag" on="submit"}}>
  <label>Name: </label>
  {{input value=name class="form-control"}}
  <button type="submit" class="btn btn-primary">Add Tag</button>
</form>

{{outlet}}
